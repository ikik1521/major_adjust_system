<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.scuec.major_adjust_system.dao.CancleTableDao">
  
  <update id="clearDate">
  		TRUNCATE TABLE `cancle_table`
  </update>
  
  <delete id="deleteByYearAndMajorCode">
  		DELETE FROM `cancle_table` WHERE `cancle_year`=#{year} AND `major_code`=#{majorCode}
  </delete>
  
  <select id="getAll" resultType="cn.scuec.major_adjust_system.model.CancleTable">
  	select * from cancle_table
  </select>
</mapper>