<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="cn.scuec.major_adjust_system.dao.UserDao">
  	
  	<select id="queryuser" resultType="cn.scuec.major_adjust_system.model.User">
  		select * from user
  	</select>
  	
  	<select id="getUserByUserName" resultType="cn.scuec.major_adjust_system.model.User">
  		SELECT * FROM `user` WHERE `name`= #{username}
  	</select>
  
  </mapper>